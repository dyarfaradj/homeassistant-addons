{
  "name": "Raspberry Pi LED Control",
  "version": "1.0.8",
  "slug": "disable_led",
  "description": "Control Raspberry Pi onboard LEDs with intuitive web interface",
  "url": "https://github.com/dyarfaradj/homeassistant-addons/tree/main/disable-led",
  "arch": ["aarch64", "armv7", "armhf", "amd64"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ingress": true,
  "ingress_port": 8099,
  "panel_icon": "mdi:led-on",
  "ingress_stream": true,
  "privileged": ["SYS_ADMIN", "SYS_RAWIO"],
  "full_access": true,
  "devices": [
    "/sys/class/leds/led0",
    "/sys/class/leds/led1",
    "/sys/class/leds/ACT",
    "/sys/class/leds/PWR",
    "/sys/class/leds/act",
    "/sys/class/leds/pwr"
  ],
  "options": {},
  "schema": {},
  "stage": "stable",
  "hassio_api": true,
  "hassio_role": "default",
  "map": ["share:rw", "ssl"]
}
